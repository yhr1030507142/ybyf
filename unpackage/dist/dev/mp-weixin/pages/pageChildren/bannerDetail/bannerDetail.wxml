<view><view class="park-box flex col"><view class="park-box-pic"><image class="img" src="{{list.primary}}" mode data-event-opts="{{[['tap',[['previewImage',['$0'],['list.primary']]]]]}}" bindtap="__e"></image></view><view class="park-box-content flex col"><view class="park-box-content-header">{{''+list.name+''}}</view><rich-text class="park-box-content-middle" nodes="{{content}}"></rich-text></view></view><view class="select"><view class="select-box flex row"><view class="select-title">姓名</view><input class="select-end select-input" style="text-align:right;" type="text" placeholder="请输入姓名" placeholder-style="text-align: right" data-event-opts="{{[['input',[['__set_model',['','title','$event',[]]]]]]}}" value="{{title}}" bindinput="__e"/></view></view><view class="select"><view class="select-box flex row"><view class="select-title">联系方式</view><input class="select-end select-input" style="text-align:right;" type="number" placeholder="请输入联系方式" placeholder-style="text-align: right" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','title_phone','$event',[]]]]]]}}" value="{{title_phone}}" bindinput="__e"/></view></view><view class="park-box-select"><button class="btn" type="primary" data-event-opts="{{[['tap',[['addData',['$event']]]]]}}" bindtap="__e">合作咨询</button></view><view class="park-box-select"><button class="btn" type="primary" data-event-opts="{{[['tap',[['apply',['$event']]]]]}}" bindtap="__e"><text class="iconfont icondianhua"></text>{{list.phone}}</button></view></view>